{
  "name": "antigram-extension",
  "version": "1.0.0",
  "description": "Antigram is a chrome extension that gives the user tools to fight back Instagram's addictive features. It was made on my free time with the objective of being able to check what were my friends up to without falling into a blackhole of meaningless content.",
  "main": "src/app/background.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./scripts/build.js",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "precommit": "npm run format && npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymyo/antigram-extension.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aymyo/antigram-extension/issues"
  },
  "homepage": "https://github.com/aymyo/antigram-extension#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "@types/chrome": "0.0.196"
  }
}
